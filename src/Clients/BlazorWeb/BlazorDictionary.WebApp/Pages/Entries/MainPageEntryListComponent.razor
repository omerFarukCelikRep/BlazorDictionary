@if (_entries is not null)
{
    foreach (var entry in _entries.Results)
    {
        <EntryViewerComponent Entry="entry" />
        <br />
    }
}

@code {
    [Inject]
    IEntryService EntryService { get; set; }

    PagedViewModel<GetEntryDetailViewModel> _entries;

    protected override async Task OnInitializedAsync()
    {
        await EntryService.GetMainPageEntries(1, 20);
    }
}
